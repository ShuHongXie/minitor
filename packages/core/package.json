{
  "name": "@minitrack/core",
  "version": "0.0.1",
  "private": true,
  "description": "监控系统核心包",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "development": {
        "types": "./src/index.ts",
        "import": "./src/index.ts",
        "require": "./src/index.ts"
      },
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./error": {
      "development": {
        "types": "./src/error/index.ts",
        "import": "./src/error/index.ts",
        "require": "./src/error/index.ts"
      },
      "types": "./dist/error/index.d.ts",
      "import": "./dist/error/index.js",
      "require": "./dist/error/index.cjs",
      "default": "./dist/error/index.js"
    },
    "./vitals": {
      "development": {
        "types": "./src/vitals/index.ts",
        "import": "./src/vitals/index.ts",
        "require": "./src/vitals/index.ts"
      },
      "types": "./dist/vitals/index.d.ts",
      "import": "./dist/vitals/index.js",
      "require": "./dist/vitals/index.cjs",
      "default": "./dist/vitals/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "devDependencies": {
    "tsup": "catalog:",
    "typescript": "catalog:"
  },
  "dependencies": {
    "defu": "^6.1.4",
    "web-vitals": "^5.1.0"
  }
}
